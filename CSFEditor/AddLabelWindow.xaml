<Window x:Class="CSFEditor.AddLabelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSFEditor"
        mc:Ignorable="d" ResizeMode="NoResize"
        Title="修改键值对" Height="342" Width="400">
    <StackPanel Margin="0,5">
        <TextBox x:Name="_tbValue" Margin="10,5" Height="192"/>
        <DockPanel Margin="10,5">
            <Label Content="标签:" Width="64"/>
            <TextBox x:Name="_tbLabel"/>
        </DockPanel>
        <DockPanel Margin="10,5">
            <Label Content="额外值:" Width="64"/>
            <TextBox x:Name="_tbEValue"/>
        </DockPanel>
        <Button Content="完成" Margin="10,5" Click="Button_Click"/>
    </StackPanel>
</Window>
